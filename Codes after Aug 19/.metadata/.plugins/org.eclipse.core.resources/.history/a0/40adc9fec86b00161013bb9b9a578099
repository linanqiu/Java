package org.javaiw.model;

import java.util.Date;

public class Employee {
	int empno;
	String ename;
	String job;
	int sal;
	Date doj;
	public Employee(){
		super();
	}
	public Employee(int empno, String ename, String job, int sal, Date doj) {
		super();
		this.empno = empno;
		this.ename = ename;
		this.job = job;
		this.sal = sal;
		this.doj = doj;
	}
	
}
