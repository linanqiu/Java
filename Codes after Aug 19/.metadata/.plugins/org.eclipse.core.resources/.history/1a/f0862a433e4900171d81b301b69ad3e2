import javax.ws.rs.core.MediaType;

@Path("account")
public class UserAccount {
	
	@GET
	@Produces(value = MediaType.TEXT_HTML)
	public String getAccounts(){
		return "<message>We are in getAccounts method </message>";
	}
	
}
